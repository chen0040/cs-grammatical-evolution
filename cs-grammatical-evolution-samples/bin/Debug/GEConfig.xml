<?xml version="1.0" ?>

<configuration>
<parameters>
<param name="NumRegisters" value="6" />
<param name="PopulationSize" value="100" />
<param name="Epsilon" value="0.000000001" />
<param name="MaxGenerations" value="1000" />
<param name="Maximization" value="false" />
<param name="CrossoverRate" value="0.8" />
<param name="MacroMutationRate" value="0.1" />
<param name="MicroMutationRate" value="0.25" />
<param name="ChromosomeLength" value="20" />
<param name="MaxParseLengthBeforeTermination" value="10" />
<param name="ChromosomeValueUpperBound" value="255" />
</parameters>

<log_parameters>
<param name="PopLogInterval" value="50" />
<param name="PopLogFilePath" value="pop_log" />
<param name="StatsLogFilePath" value="stats_log" />
<param name="RunLogFilePath" value="run_log" />
</log_parameters>

<lgp_scripts>
<script name="CrossoverInstructionFactory" src="GEConfig\CrossoverInstructionFactory.xml" />
<script name="MutationInstructionFactory" src="GEConfig\MutationInstructionFactory.xml" />
<script name="PopInitInstructionFactory" src="GEConfig\PopInitInstructionFactory.xml" />
<script name="RegInitInstructionFactory" src="GEConfig\RegInitInstructionFactory.xml" />
<script name="ReproductionSelectionInstructionFactory" src="GEConfig\ReproductionSelectionInstructionFactory.xml" />
<script name="SurvivalInstructionFactory" src="GEConfig\SurvivalInstructionFactory.xml" />
</lgp_scripts>

</configuration>